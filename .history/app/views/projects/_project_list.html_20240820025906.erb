<!-- app/views/projects/_project_list.html.erb -->
<% projects.each do |project| %>
  <div class="project-item">
    <h3><%= project.name %></h3>
    <p><%= project.description %></p>
    <!-- Add more project details as needed -->
    <%= link_to "View Project", project_path(project) %>
  </div>
<% end %>
